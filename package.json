{
  "name": "npm-react-module",
  "version": "1.0.0",
  "description": "Easy customizable ReactJS module boilerplate for npm",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "dev": "webpack --config ./examples/webpack.config.js",
    "lint": "eslint src || exit 0",
    "build": "cross-env babel src --out-dir lib",
    "copy": "cp src/style.css lib/",
    "prepublish": "npm run clean && npm run build && npm run copy"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lassegit/npm-react-module"
  },
  "keywords": [
    "boilerplate",
    "react",
    "npm"
  ],
  "author": "Lasse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lassegit/npm-react-module/issues"
  },
  "homepage": "https://github.com/lassegit/npm-react-module",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.13.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
